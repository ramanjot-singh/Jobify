<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#a1a100" />

            <stroke android:width="1dp" android:color="#8f8f00" />

            <corners android:radius="3dp" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270" android:endColor="#b3b300" android:startColor="#b3b300" />

            <stroke android:width="1dp" android:color="#FFDB58" />

            <corners android:radius="3dp" />
        </shape></item>

</selector>